{
    "name": "noxoua/filament-activity-log",
    "description": "This is my package filament-activity-log",
    "keywords": [
        "noxoua",
        "laravel",
        "filament-activity-log"
    ],
    "homepage": "https://github.com/noxoua/filament-activity-log",
    "support": {
        "issues": "https://github.com/noxoua/filament-activity-log/issues",
        "source": "https://github.com/noxoua/filament-activity-log"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noxo",
            "email": "noxoua@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0",
        "spatie/laravel-activitylog": "^4.7"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Noxo\\FilamentActivityLog\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noxo\\FilamentActivityLog\\FilamentActivityLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
